{
    "name": "uni-ai",
    "version": "1.1.0",
    "description": "UniAI integrates multiple AI models and provide model as a service APIs",
    "egg": {
        "typescript": true
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-openai-app --port 3300",
        "stop": "egg-scripts stop --title=egg-server-openai-app",
        "dev": "egg-bin dev --port 3300",
        "ets": "ets",
        "test-local": "egg-bin test -p",
        "test": "npm run lint -- --fix && npm run test-local",
        "cov": "egg-bin cov -p",
        "ci": "npm run lint && npm run cov && npm run tsc && npm run clean",
        "lint": "eslint . --ext .ts --cache",
        "tsc": "tsc",
        "build": "tsc",
        "clean": "tsc -b --clean",
        "pg": "ts-node ./script/sequelize.ts",
        "docker": "ts-node ./docker/docker.ts"
    },
