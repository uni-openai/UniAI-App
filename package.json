        "@eggjs/tegg-eventbus-plugin": "^3.5.2",
        "@eggjs/tegg-plugin": "^3.5.2",
        "@eggjs/tegg-schedule-plugin": "^3.5.2",
        "@openai-link/egg-sequelize-ts": "^1.1.1",
        "@stdlib/assert-is-json": "^0.0.8",
        "@tensorflow/tfjs-node": "^4.2.0",
        "@zilliz/milvus2-sdk-node": "^2.2.4",
        "axios": "^1.3.4",
        "cld": "^2.9.0",
        "cos-nodejs-sdk-v5": "^2.11.19",
        "dotenv": "^16.0.3",
        "egg": "^3.15.0",
        "egg-cors": "^2.2.3",
        "egg-multipart": "^3.3.0",
        "egg-scripts": "^2.17.0",
        "egg-sequelize": "^6.0.0",
        "egg-tracer": "^2.0.0",
        "egg-view-nunjucks": "^2.3.0",
        "eventsource": "^2.0.2",
        "file-type": "16.5.4",
        "gpt-3-encoder": "^1.1.4",
        "ioredis": "^5.3.1",
        "leoric": "^2.9.2",
        "lodash": "^4.17.21",
