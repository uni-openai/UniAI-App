{
    "name": "maas-app",
    "version": "1.0.0",
    "description": "Model as a Service backend template, use egg to create AI apps",
    "private": true,
    "egg": {
        "typescript": true
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-openai-app --port 3000",
        "stop": "egg-scripts stop --title=egg-server-openai-app",
        "dev": "egg-bin dev --port 3000",
        "ets": "ets",
        "test-local": "egg-bin test -p",
        "test": "npm run lint -- --fix && npm run test-local",
        "cov": "egg-bin cov -p",
        "ci": "npm run lint && npm run cov && npm run tsc && npm run clean",
        "lint": "eslint . --ext .ts --cache",
        "tsc": "tsc",
        "build": "tsc",
        "clean": "tsc -b --clean",
        "milvus": "ts-node ./script/milvus.ts",
        "sequelize": "ts-node ./script/sequelize.ts",
        "pg": "ts-node ./script/sequelize.ts",
        "pgvector": "ts-node ./script/sequelize.ts",
        "docker": "ts-node ./docker/docker.ts"
    },
    "dependencies": {
        "@eggjs/tegg": "^3.5.2",
        "@eggjs/tegg-aop-plugin": "^3.5.2",
        "@eggjs/tegg-config": "^3.2.3",
