        "pg-hstore": "^2.3.4",
        "qs": "^6.11.1",
        "reflect-metadata": "^0.1.13",
        "sbd": "^1.0.19",
        "seedrandom": "^3.0.5",
        "sentence-splitter": "^4.2.0",
